{
  "name": "lnl-calc",
  "version": "1.0.0",
  "description": "A Latitude and Longitude calculator for geographic coordinate conversions and calculations",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "ava",
    "test:watch": "ava --watch",
    "deploy": "gh-pages -d dist",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "keywords": [
    "latitude",
    "longitude",
    "coordinates",
    "gps",
    "geographic",
    "mapping",
    "navigation",
    "lit",
    "web-components",
    "spectrum"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com/)",
  "license": "Apache-2.0",
  "dependencies": {
    "@spectrum-web-components/bundle": "1.7.0",
    "lit": "3.3.1"
  },
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "24.0.14",
    "@web/test-runner": "0.20.2",
    "@web/test-runner-playwright": "0.11.1",
    "ava": "6.4.1",
    "gh-pages": "6.3.0",
    "husky": "^9.1.7",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "vite": "7.0.5"
  }
}
